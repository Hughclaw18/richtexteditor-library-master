<div
  class="ui-rte-suggestion-item-list ui-rte-atmetion-suggestion-container zdc_shareautocompletedialog"
>
  {{#each suggestions}}
    <div class="ui-rte-suggestion-item">
      <div
        style="height:30px;width:30px;float:left;margin-right: 10px;margin-left: 10px;margin-top: 10px;"
        class=""
      >
        <img
          src="https://contacts.zoho.com/file?ID={{this.zuid}}&fs=thumb"
          style="border-radius: 100%;height:35px;width:35px;border:1px solid #e0e0e0;"
          id="contact-img"
          title=""
        />
      </div>
      <div class="" style="float:left;padding:3px;margin-top:5px">
        <div style="" class="">
          <a
            style="white-space:nowrap;"
            class="ui-corner-all ui-cmt-atmention-fname"
            tabindex="-1"
          ><span
              class="ui-rte-cmnt-atmention-name"
              id="full-name"
            >{{this.fullname}}</span></a>
        </div>
        <div class="">
          <span class="graytxt" id="graytxt">{{this.emailid}}&nbsp;</span>
        </div>
      </div>
    </div>
  {{/each}}
</div>